/* page/UI/pages/scroll-op/scroll-op.wxss */

.card {
  width: 100%;
  background-color: #fff;
  border-radius: 10rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.card-hd {
  width: 90%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  margin: 16rpx 0;
}

.card-title {
  color: #333;
  font-size: 27rpx;
}

.card-op {
  color: #7f7f7f;
  font-size: 24rpx;
}

.card-bd {
  width: 90%;
  margin:0;
}

.card-bd swiper-item {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}

.icon {
  margin: 10rpx 0;
}

.icon-name {
  font-size: 24rpx;
}

swiper {
  height: 100px;
  margin: 0;
}

